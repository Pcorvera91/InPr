procedure EscribirFecha___(dia, mes, año) {
    /*
        PROPÓSITO: Escribe una fecha con un dia, un mes y un año
        dados
        PARÁMETROS:
            * dia: numero - representa el dia con bolitas azules.
            * mes: numero - representa el mes con bolitas rojas.
            * año: numero - representa el año con bolitas verdes.
        PRECONDICIONES:
            * dia debe ser entre 1 y 31
            *mes debe ser entre 1 y 12
            *al menos debe haber 2 celdas lindantes de la actual
            *tanto la celda actual como las 2 lindantes deben
            estar vacias.
    */
    EscribirDia_(dia)
    Mover(Este)
    EscribirMes_(mes)
    Mover(Este)
    EscribirAnio(año)
}

procedure EscribirDia_(dia){
    /*
        PROPÓSITO:  Escribe el dia representando con bolitas azules
        PARÁMETROS:
            * dia: numero - es la cantidad de bolitas azules a poner
            para representar el dia.
            
        PRECONDICIONES:
            * la celda actual debe estar vacia
    */
    repeat (dia) {
        Poner(Azul)
    }
}

procedure EscribirMes_(mes){
    /*
        PROPÓSITO:  Escribe el mes representando con bolitas rojas
        PARÁMETROS:
            * mes: numero - es la cantidad de bolitas rojas a poner
            para representar el mes.
            
        PRECONDICIONES:
            * la celda actual debe estar vacia
    */
    repeat (mes) {
        Poner(Rojo)
    }
}

procedure EscribirAnio_(año){
    /*
        PROPÓSITO:  Escribe el año representando con bolitas Verdes
        PARÁMETROS:
            * año: numero - es la cantidad de bolitas Verdes a poner
            para representar el año.
            
        PRECONDICIONES:
            * la celda actual debe estar vacia
    */
    repeat (año) {
        Poner(Verde)
    }
}

